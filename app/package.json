{
  "name": "seer-robot-app",
  "version": "1.0.0",
  "description": "SEER blockchain",
  "main": "src/index.js",
  "author": "SEER blockchain",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir ELECTRON_ENV=production",
    "setup": "electron-builder",
    "postintall": "cnpm install --arch=ia32"
  },
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-serve": "^0.2.0",
    "lodash": "^4.17.11",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "node-schedule": "^1.3.0"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^20.28.2",
    "electron-prebuilt-compile": "^2.0.7",
    "electron-updater": "^3.1.1"
  },
  "build": {
    "appId": "SEER_ROBOT",
    "productName": "SEER机器人",
    "electronCompile": true,
    "extraFiles": [
      "data"
    ],
    "files": [
      "src",
      "app"
    ],
    "win":{
      "icon":"app/launch.ico"
    },
    "copyright": "Copyright © 2018 SEER blockchain",
    "publish": [
      {
        "provider": "generic",
        "url": "http://localhost:8080/download/"
      }
    ]
  }
}
